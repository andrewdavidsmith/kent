    priority 2

    track srav3h
    shortLabel SRA
    longLabel recount 3 SRA introns
    bigDataUrl /gbdb/$D/recount3/srav3h.bb
    filter.readcount 10000:2000000000
    labelFields none
    maxWindowToDraw 10000000
    mouseOver $readcount supporting split reads<br>Strand $strand<br>Size $size<br>Splice donor $donor<br>Splice acceptor $acceptor
    iframeUrl https://snaptron.cs.jhu.edu/snaptron-studies/jxn2studies?compilation=srav3h&jid=$$&coords=$S:${-$}
    iframeOptions height='300' width='1000' scrolling='yes'
    decorator.default.bigDataUrl /gbdb/$D/recount3/decsrav3h.bb
    parent recount3
    priority 3

    track ccle
    shortLabel CCLE
    longLabel recount 3 CCLE introns
    bigDataUrl /gbdb/$D/recount3/ccle.bb
